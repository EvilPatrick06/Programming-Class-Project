{
    "name": "repo-auto-sync",
    "displayName": "Repository Auto-Sync",
    "description": "Automatically syncs repository when codespace opens",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": ["Other"],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "repo-auto-sync.sync",
                "title": "Repository Auto-Sync"
            }
        ]
    }
}